{
  "manifest_version": 3,
  "name": "AutoPly",
  "version": "1.0.0",
  "description": "Auto Apply To Jobs.",
  "permissions": ["scripting", "tabs", "storage", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "pictures/logo2.png",
      "48": "pictures/logo2.png",
      "128": "pictures/logo2.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["router.js"],
      "run_at": "document_idle",
      "type": "module"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["apply1.js", "apply2.js", "createAccount.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "pictures/logo2.png",
    "48": "pictures/logo2.png",
    "128": "pictures/logo2.png"
  }
}
