{
    "manifest_version": 3,
    "name": "AutoPly",
    "version": "1.0.0",
    "description": "Auto Apply To Jobs.",
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "pictures/temp.png",
        "48": "pictures/temp.png",
        "128": "pictures/temp.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "16": "pictures/temp.png",
      "48": "pictures/temp.png",
      "128": "pictures/temp.png"
    }
  }
  